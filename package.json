{
    "name": "typesense-nestjs",
    "version": "1.1.3",
    "description": "Typesence module for NestJS and sync data from monoDB",
    "main": "build/index",
    "types": "build/index",
    "scripts": {
        "build": "nest build typesense-nestjs-lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hainguyen27798/typesense-nestjs.git"
    },
    "author": "Harry Nguyen",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/hainguyen27798/typesense-nestjs/issues"
    },
    "homepage": "https://github.com/hainguyen27798/typesense-nestjs#readme",
    "keywords": [
        "typesense",
        "api",
        "wrapper",
        "nestjs",
        "search"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "peerDependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/mongoose": "^10.0.6",
        "mongoose": "^8.3.2",
        "rxjs": "^7.8.1",
        "typesense": "^1.8.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/mongoose": "^10.0.6",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.4.17",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-simple-import-sort": "^12.1.0",
        "eslint-plugin-unused-imports": "^3.1.0",
        "jest": "^29.5.0",
        "lodash": "^4.17.21",
        "mongoose": "^8.3.2",
        "prettier": "^3.0.0",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3",
        "typesense": "^1.8.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/src/",
            "<rootDir>/libs/"
        ],
        "moduleNameMapper": {
            "^@app/typesense-nestjs-lib(|/.*)$": "<rootDir>/build/$1"
        }
    }
}
